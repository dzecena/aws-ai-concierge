    "\n",
    "Respond in a professional, helpful manner while being concise and focused on practical solutions.'''\n",
    "        )\n",
    "        \n",
    "        agent_id = agent_response['agent']['agentId']\n",
    "        print(f\"\\n‚úÖ Created Bedrock Agent: {agent_id}\")\n",
    "        \n",
    "        # Create action group\n",
    "        action_group_response = bedrock_agent.create_agent_action_group(\n",
    "            agentId=agent_id,\n",
    "            agentVersion='DRAFT',\n",
    "            actionGroupName='aws-ai-concierge-tools',\n",
    "            description='AWS infrastructure analysis and management tools',\n",
    "            actionGroupExecutor={\n",
    "                'lambda': lambda_arn\n",
    "            },\n",
    "            apiSchema={\n",
    "                's3': {\n",
    "                    's3BucketName': openapi_bucket,\n",
    "                    's3ObjectKey': 'aws-ai-concierge-tools.yaml'\n",
    "                }\n",
    "            }\n",
    "        )\n",
    "        \n",
    "        print(f\"‚úÖ Created Action Group: {action_group_response['agentActionGroup']['actionGroupId']}\")\n",
    "        \n",
    "        # Prepare the agent\n",
    "        print(\"üîÑ Preparing agent...\")\n",
    "        prepare_response = bedrock_agent.prepare_agent(\n",
    "            agentId=agent_id\n",
    "        )\n",
    "        \n",
    "        # Wait for preparation to complete\n",
    "        max_attempts = 30\n",
    "        for attempt in range(max_attempts):\n",
    "            agent_status = bedrock_agent.get_agent(\n",
    "                agentId=agent_id\n",
    "            )\n",
    "            \n",
    "            status = agent_status['agent']['agentStatus']\n",
    "            print(f\"Agent status: {status} (attempt {attempt + 1}/{max_attempts})\")\n",
    "            \n",
    "            if status == 'PREPARED':\n",
    "                print(\"\\nüéâ Bedrock Agent is ready!\")\n",
    "                break\n",
    "            elif status == 'FAILED':\n",
    "                print(\"\\n‚ùå Agent preparation failed\")\n",
    "                break\n",
    "            \n",
    "            time.sleep(10)\n",
    "        \n",
    "        return agent_id\n",
    "        \n",
    "    except Exception as e:\n",
    "        print(f\"‚ùå Error creating Bedrock Agent: {e}\")\n",
    "        return None\n",
    "\n",
    "# Create the agent\n",
