    "# Create security assessment tool\n",
    "security_assessment_code = '''import boto3\n",
    "import json\n",
    "from utils.aws_clients import get_aws_client\n",
    "from utils.error_handler import handle_error\n",
    "\n",
    "def assess_security(parameters):\n",
    "    \"\"\"Assess AWS security posture\"\"\"\n",
    "    try:\n",
    "        region = parameters.get('region', 'us-east-1')\n",
    "        assessment_type = parameters.get('assessment_type', 'security_groups')\n",
    "        \n",
    "        findings = []\n",
    "        \n",
    "        if assessment_type in ['all', 'security_groups']:\n",
    "            ec2_client = get_aws_client('ec2', region)\n",
    "            security_groups = ec2_client.describe_security_groups()\n",
    "            \n",
    "            for sg in security_groups['SecurityGroups']:\n",
    "                # Check for overly permissive rules\n",
    "                for rule in sg.get('IpPermissions', []):\n",
    "                    for ip_range in rule.get('IpRanges', []):\n",
    "                        if ip_range.get('CidrIp') == '0.0.0.0/0':\n",
    "                            findings.append({\n",
    "                                'type': 'security_group',\n",
    "                                'severity': 'HIGH',\n",
    "                                'resource': sg['GroupId'],\n",
    "                                'description': f\"Security group {sg['GroupName']} allows inbound traffic from anywhere (0.0.0.0/0)\",\n",
    "                                'recommendation': \"Restrict source IP ranges to specific networks\"\n",
    "                            })\n",
    "        \n",
    "        # Check S3 bucket policies\n",
    "        if assessment_type in ['all', 's3_buckets']:\n",
    "            s3_client = get_aws_client('s3')\n",
    "            buckets = s3_client.list_buckets()\n",
    "            \n",
    "            for bucket in buckets['Buckets']:\n",
    "                try:\n",
    "                    # Check public access\n",
    "                    public_access = s3_client.get_public_access_block(\n",
    "                        Bucket=bucket['Name']\n",
    "                    )\n",
    "                    \n",
    "                    config = public_access['PublicAccessBlockConfiguration']\n",
    "                    if not all([\n",
    "                        config.get('BlockPublicAcls', False),\n",
    "                        config.get('IgnorePublicAcls', False),\n",
    "                        config.get('BlockPublicPolicy', False),\n",
    "                        config.get('RestrictPublicBuckets', False)\n",
    "                    ]):\n",
    "                        findings.append({\n",
    "                            'type': 's3_bucket',\n",
    "                            'severity': 'MEDIUM',\n",
    "                            'resource': bucket['Name'],\n",
    "                            'description': f\"S3 bucket {bucket['Name']} may allow public access\",\n",
    "                            'recommendation': \"Enable all public access block settings\"\n",
    "                        })\n",
    "                        \n",
    "                except Exception:\n",
    "                    # Bucket might not have public access block configured\n",
    "                    findings.append({\n",
    "                        'type': 's3_bucket',\n",
    "                        'severity': 'LOW',\n",
    "                        'resource': bucket['Name'],\n",
    "                        'description': f\"S3 bucket {bucket['Name']} public access block not configured\",\n",
    "                        'recommendation': \"Configure public access block settings\"\n",
    "                    })\n",
    "        \n",
    "        # Categorize findings by severity\n",
    "        severity_counts = {'HIGH': 0, 'MEDIUM': 0, 'LOW': 0}\n",
    "        for finding in findings:\n",
    "            severity_counts[finding['severity']] += 1\n",
    "        \n",
    "        return {\n",
    "            'region': region,\n",
    "            'assessment_type': assessment_type,\n",
    "            'findings': findings,\n",
    "            'summary': {\n",
    "                'total_findings': len(findings),\n",
    "                'severity_breakdown': severity_counts\n",
    "            }\n",
    "        }\n",
    "        \n",
    "    except Exception as e:\n",
    "        return handle_error(e, \"Security assessment failed\")\n",
    "'''\n",
    "\n",
    "with open('lambda-src/tools/security_assessment.py', 'w') as f:\n",
    "    f.write(security_assessment_code)\n",
    "    \n",
    "print(\"‚úÖ Created lambda-src/tools/security_assessment.py\")"
   ]
