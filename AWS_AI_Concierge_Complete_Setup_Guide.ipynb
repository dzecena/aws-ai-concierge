   "source": [
    "# Create cost analysis tool\n",
    "cost_analysis_code = '''import boto3\n",
    "import json\n",
    "from datetime import datetime, timedelta\n",
    "from utils.aws_clients import get_aws_client\n",
    "from utils.error_handler import handle_error\n",
    "\n",
    "def get_cost_analysis(parameters):\n",
    "    \"\"\"Analyze AWS costs using Cost Explorer API\"\"\"\n",
    "    try:\n",
    "        ce_client = get_aws_client('ce')\n",
    "        \n",
    "        # Parse time period\n",
    "        time_period = parameters.get('time_period', 'monthly')\n",
    "        \n",
    "        # Calculate date range\n",
    "        end_date = datetime.now().date()\n",
    "        if time_period == 'monthly':\n",
    "            start_date = end_date.replace(day=1)\n",
    "        elif time_period == 'weekly':\n",
    "            start_date = end_date - timedelta(days=7)\n",
    "        else:\n",
    "            start_date = end_date - timedelta(days=30)\n",
    "        \n",
    "        # Get cost and usage data\n",
    "        response = ce_client.get_cost_and_usage(\n",
    "            TimePeriod={\n",
    "                'Start': start_date.strftime('%Y-%m-%d'),\n",
    "                'End': end_date.strftime('%Y-%m-%d')\n",
    "            },\n",
    "            Granularity='MONTHLY',\n",
    "            Metrics=['BlendedCost'],\n",
    "            GroupBy=[\n",
    "                {'Type': 'DIMENSION', 'Key': 'SERVICE'}\n",
    "            ]\n",
    "        )\n",
    "        \n",
    "        # Process results\n",
    "        cost_data = []\n",
    "        total_cost = 0\n",
    "        \n",
    "        for result in response['ResultsByTime']:\n",
    "            for group in result['Groups']:\n",
    "                service = group['Keys'][0]\n",
    "                amount = float(group['Metrics']['BlendedCost']['Amount'])\n",
    "                if amount > 0:\n",
    "                    cost_data.append({\n",
    "                        'service': service,\n",
    "                        'cost': amount,\n",
    "                        'currency': group['Metrics']['BlendedCost']['Unit']\n",
    "                    })\n",
    "                    total_cost += amount\n",
    "        \n",
    "        # Sort by cost descending\n",
    "        cost_data.sort(key=lambda x: x['cost'], reverse=True)\n",
    "        \n",
    "        return {\n",
    "            'total_cost': total_cost,\n",
    "            'period': f\"{start_date} to {end_date}\",\n",
    "            'services': cost_data[:10],  # Top 10 services\n",
    "            'currency': 'USD'\n",
    "        }\n",
    "        \n",
    "    except Exception as e:\n",
    "        return handle_error(e, \"Cost analysis failed\")\n",
    "'''\n",
    "\n",
    "with open('lambda-src/tools/cost_analysis.py', 'w') as f:\n",
    "    f.write(cost_analysis_code)\n",
    "    \n",
    "print(\"‚úÖ Created lambda-src/tools/cost_analysis.py\")"
   ]
