    "# Create resource discovery tool\n",
    "resource_discovery_code = '''import boto3\n",
    "import json\n",
    "from utils.aws_clients import get_aws_client\n",
    "from utils.error_handler import handle_error\n",
    "\n",
    "def discover_resources(parameters):\n",
    "    \"\"\"Discover AWS resources across multiple services\"\"\"\n",
    "    try:\n",
    "        resource_type = parameters.get('resource_type', 'all')\n",
    "        region = parameters.get('region', 'us-east-1')\n",
    "        \n",
    "        resources = {}\n",
    "        \n",
    "        # EC2 Instances\n",
    "        if resource_type in ['all', 'ec2']:\n",
    "            ec2_client = get_aws_client('ec2', region)\n",
    "            instances = ec2_client.describe_instances()\n",
    "            \n",
    "            ec2_list = []\n",
    "            for reservation in instances['Reservations']:\n",
    "                for instance in reservation['Instances']:\n",
    "                    ec2_list.append({\n",
    "                        'id': instance['InstanceId'],\n",
    "                        'type': instance['InstanceType'],\n",
    "                        'state': instance['State']['Name'],\n",
    "                        'launch_time': instance['LaunchTime'].isoformat()\n",
    "                    })\n",
    "            resources['ec2_instances'] = ec2_list\n",
    "        \n",
    "        # S3 Buckets\n",
    "        if resource_type in ['all', 's3']:\n",
    "            s3_client = get_aws_client('s3')\n",
    "            buckets = s3_client.list_buckets()\n",
    "            \n",
    "            s3_list = []\n",
    "            for bucket in buckets['Buckets']:\n",
    "                s3_list.append({\n",
    "                    'name': bucket['Name'],\n",
    "                    'creation_date': bucket['CreationDate'].isoformat()\n",
    "                })\n",
    "            resources['s3_buckets'] = s3_list\n",
    "        \n",
    "        # RDS Instances\n",
    "        if resource_type in ['all', 'rds']:\n",
    "            rds_client = get_aws_client('rds', region)\n",
    "            db_instances = rds_client.describe_db_instances()\n",
    "            \n",
    "            rds_list = []\n",
    "            for db in db_instances['DBInstances']:\n",
    "                rds_list.append({\n",
    "                    'id': db['DBInstanceIdentifier'],\n",
    "                    'engine': db['Engine'],\n",
    "                    'status': db['DBInstanceStatus'],\n",
    "                    'class': db['DBInstanceClass']\n",
    "                })\n",
    "            resources['rds_instances'] = rds_list\n",
    "        \n",
    "        return {\n",
    "            'region': region,\n",
    "            'resource_type': resource_type,\n",
    "            'resources': resources,\n",
    "            'total_count': sum(len(v) if isinstance(v, list) else 0 for v in resources.values())\n",
    "        }\n",
    "        \n",
    "    except Exception as e:\n",
    "        return handle_error(e, \"Resource discovery failed\")\n",
    "'''\n",
    "\n",
    "with open('lambda-src/tools/resource_discovery.py', 'w') as f:\n",
    "    f.write(resource_discovery_code)\n",
    "    \n",
    "print(\"‚úÖ Created lambda-src/tools/resource_discovery.py\")"
